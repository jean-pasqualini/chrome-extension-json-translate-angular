{

  "manifest_version": 2,

  "name": "Json translate helper",

  "description": "This extension help translate i18n writted in json",

  "version": "1.0",

  "background" : {
    "scripts" : ["bg.js"]
  },

  "permissions": [

    "tabs"

  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "icons" : {"48": "icon.png"},

  "browser_action": {

    "default_icon": "icon.png"

  }

}
